{
  "name": "next.js_template",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyz:build": "cross-env ANALYZE=true next build",
    "build": "next build",
    "dev": "next dev",
    "export": "yarn build && next export",
    "lint": "eslint -c ./.eslintrc.js --print-config ./.eslintrc.js | eslint-config-prettier-check && eslint -c ./.eslintrc.js '**/*.{ts,tsx}'",
    "lint:fix": "yarn run lint -- --fix && yarn rustywind --write .",
    "start": "next start",
    "test": "jest ./tests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,md,scss}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "legacyBrowsers": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "modernBrowsers": [
      "last 2 Chrome versions",
      "not Chrome < 60",
      "last 2 Safari versions",
      "not Safari < 10.1",
      "last 2 iOS versions",
      "not iOS < 10.3",
      "last 2 Firefox versions",
      "not Firefox < 54",
      "last 2 Edge versions",
      "not Edge < 15"
    ]
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.12.1",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@tailwindcss/typography": "^0.3.1",
    "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
    "@vercel/fetch": "^6.1.0",
    "classnames": "^2.2.6",
    "css-declaration-sorter": "^6.0.2",
    "lodash.map": "^4.6.0",
    "next": "^10.0.0",
    "next-plugin-preact": "^3.0.3",
    "next-seo": "^4.14.1",
    "next-themes": "^0.0.10",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.2.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "13.0.0",
    "postcss-nested": "5.0.1",
    "postcss-preset-env": "^6.7.0",
    "preact": "^10.5.5",
    "preact-markup": "^2.0.0",
    "preact-render-to-string": "^5.1.11",
    "preact-ssr-prepass": "^1.1.2",
    "preact-transitioning": "^1.0.2",
    "react": "github:preact-compat/react#1.0.0",
    "react-dom": "github:preact-compat/react-dom#1.0.0",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.1.2",
    "resolve-url-loader": "^3.1.2",
    "rosetta": "^1.1.0",
    "swr": "^0.3.7",
    "tailwindcss": "2.0.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.0.0",
    "@next/bundle-analyzer": "^10.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/preact": "^2.0.0",
    "@testing-library/preact-hooks": "^1.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/animejs": "^3.1.2",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/webpack": "^4.41.24",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.0",
    "next-with-sitemap": "^2.5.2",
    "next-with-workbox": "^2.0.1",
    "postcss-preset-env": "^6.7.0",
